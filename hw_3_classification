        "enc.fit(X_train, y_train)\n",
        "X_train_transformed = enc.transform(X_train, y_train)\n",
        "\n",
        "# fitting model\n",
        "lr = LogisticRegression()\n",
        "lr.fit(X_train_transformed, y_train)\n",
        "\n",
        "# predicting and finding AUC-PR\n",
